#include "OP.h"
#include "main.h"
#include "IP.h"
/*
 Session39 Implementations
 name:
 Robot_Controlled_via_Bluetooth
 */
int main()
{
	//MOtor1 to move CW

	//Enable RCC
	RCC->APB2ENR=(1<<2|(1<<3));

	//CONFIG AND SET
	OP M1_in1(GPIOA,5);
	OP M1_in2(GPIOA,1);
	OP M1_EN(GPIOA,6);


	while (1)
	{
		M1_in1.ON();
		M1_in2.OFF();
		M1_EN.ON();
	}

return 0;
}
